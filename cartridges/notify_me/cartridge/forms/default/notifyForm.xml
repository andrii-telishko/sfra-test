<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19">
	<field
			formid="email" 
			label="label.input.email.notifyFotm" 
			mandatory="true" 
			max-length="50" 
			missing-error="error.message.required"
			parse-error="error.message.parse.email.notifyFor.form"
			range-error="error.message.50orless"
			regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$" 
			value-error="ValueErrorText"
			type="string"/> 
<field
			formid="productId" 
			label="label.input.productId.notifyForm" 
			mandatory="true" 
			max-length="50" 
			missing-error="error.message.required"
			value-error="ValueErrorText"
			type="string"/> 
</form>